<mvc:View controllerName="unit901.controller.Main"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">
    <Page id="page" title="{i18n>title}">
        <!-- <content /> -->
        <Input id="inpFname" value="{/firstName}" editable="{/editable}"></Input>
        <Input id="inpLname" value="{/lastName}" editable="{/editable}"></Input>

        <CheckBox id="chkBox" text="All" selected="{/checked}"></CheckBox>
    </Page>
</mvc:View>
